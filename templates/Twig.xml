<templateSet group="Twig">
  <template name="ifdemo" value="{% if exportValue != &quot;demo&quot; %}&#10;    {content()}&#9;&#9;&#9;&#10;{% else %}&#10;    &lt;!-- demo content here --&gt;&#10;{% endif %}" description="demo" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="blk" value="{% block $NAME$ %}&#10;    $END$&#10;{% endblock %}&#10;" description="block...endblock" toReformat="true" toShortenFQNames="false" deactivated="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>